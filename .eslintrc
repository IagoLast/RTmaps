{
	"env": {
		"node": true,
		"es6":true,
	},
	"rules": {
		"comma-dangle": [2, "always-multiline"],
		"comma-style": [1, "last"],
		"no-catch-shadow": 2,
		"no-delete-var": 2,
		"no-extra-semi": 2,
		"no-label-var": 2,
		"no-shadow": 2,
		"no-shadow-restricted-names": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-undefined": 2,
		"no-underscore-dangle": 0,
		"no-unused-vars": 2,
		"no-use-before-define": [2, "nofunc"],
		"quotes": [2, "single"],
		"semi": [2, "always"],
		"yoda": 2,
	},
	"globals": {
    	"describe": true,
    	"it": true,
  	}
}
